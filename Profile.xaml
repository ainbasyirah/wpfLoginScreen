<Window x:Class="Wpflogin1.profile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpflogin1"
        mc:Ignorable="d"
        Title="profile" Height="450" Width="800" Background="AntiqueWhite">
    <Grid>
        <DataGrid Name="g1" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Name}" Header="Name" Width="100"/>
                <DataGridTextColumn Binding="{Binding Age}" Header="Age" Width="100"/>
                <DataGridTextColumn Binding="{Binding Address}" Header="Address" Width="100"/>
                <DataGridTextColumn Binding="{Binding Identical/nNumeric}" Header="IC" Width="100" />
            </DataGrid.Columns>

        </DataGrid>
        <Button x:Name="btnback1" Click="btnback1_Click" Content="Back" Margin="41,339,641.333,24.667" Background="DimGray" Foreground="White" FontSize="20"/>
    </Grid>
</Window>
